{
  "compilerOptions": {
    "target": "ES2021",
    "module": "es2022",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
  "noEmit": false
  },
  "include": ["**/*.ts"]
}